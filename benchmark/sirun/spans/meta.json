{
  "name": "spans",
  "run": "taskset -c 37 node spans.js",
  "cachegrind": false,
  "iterations": 10,
  "variants": {
    "finish-immediately": {
      "env": {
        "DD_TRACE_SCOPE": "noop",
        "FINISH": "now"
      }
    },
    "finish-later": {
      "env": {
        "DD_TRACE_SCOPE": "noop",
        "FINISH": "later"
      }
    }
  }
}
