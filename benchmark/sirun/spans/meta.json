{
  "name": "spans",
  "run": "node spans.js",
  "run_with_affinity": "bash -c \"taskset -c $CPU_AFFINITY node spans.js\"",
  "cachegrind": false,
  "iterations": 20,
  "variants": {
    "finish-immediately": {
      "env": {
        "DD_TRACE_SCOPE": "noop",
        "FINISH": "now"
      }
    },
    "finish-later": {
      "env": {
        "DD_TRACE_SCOPE": "noop",
        "FINISH": "later"
      }
    }
  }
}
