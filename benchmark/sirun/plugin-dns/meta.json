{
  "name": "plugin-dns",
  "run": "node index.js",
  "run_with_affinity": "bash -c \"taskset -c $CPU_AFFINITY node index.js\"",
  "cachegrind": false,
  "iterations": 20,
  "variants": {
    "control": {
      "env": { "USE_TRACER": "0" }
    },
    "with-tracer": {
      "baseline": "control",
      "env": { "USE_TRACER": "1" }
    }
  }
}
