{
  "name": "startup",
  "run": "taskset -c 38 node startup-test.js",
  "cachegrind": false,
  "iterations": 10,
  "variants": {
    "control": {
      "env": {
        "USE_TRACER": "0"
      }
    },
    "with-tracer": {
      "baseline": "control",
      "env": {
        "USE_TRACER": "1"
      }
    },
    "control-everything": {
      "env": {
        "USE_TRACER": "0",
        "EVERYTHING": "1"
      }
    },
    "with-tracer-everything": {
      "baseline": "control-everything",
      "env": {
        "USE_TRACER": "1",
        "EVERYTHING": "1"
      }
    }
  }
}
