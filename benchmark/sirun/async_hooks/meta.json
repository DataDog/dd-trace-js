{
  "name": "async_hooks",
  "run": "node -r ../monitor .",
  "run_with_affinity": "bash -c \"taskset -c $CPU_AFFINITY node -r ../monitor .\"",
  "cachegrind": false,
  "iterations": 5,
  "variants": {
    "no-hooks": { "env": { "ASYNC_HOOKS": "" } },
    "init-only": {
      "baseline": "no-hooks",
      "env": { "ASYNC_HOOKS": "init" }
    },
    "all-hooks": {
      "baseline": "no-hooks",
      "env": { "ASYNC_HOOKS": "init,before,after,destroy,promiseResolve" }
    }
  }
}
