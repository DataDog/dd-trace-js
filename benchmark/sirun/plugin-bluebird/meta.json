{
  "name": "plugin-bluebird",
  "run": "node index.js",
  "run_with_affinity": "bash -c \"taskset -c $CPU_AFFINITY node index.js\"",
  "cachegrind": false,
  "iterations": 10,
  "variants": {
    "control": {
      "env": {
        "USE_TRACER": "0",
        "COUNT": "50000"
      }
    },
    "with-tracer": {
      "baseline": "control",
      "env": {
        "USE_TRACER": "1",
        "COUNT": "50000"
      }
    }
  }
}
