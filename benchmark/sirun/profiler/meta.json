{
  "name": "profiler",
  "run": "node -r ../monitor index.js",
  "run_with_affinity": "bash -c \"taskset -c $CPU_AFFINITY node -r ../monitor index.js\"",
  "cachegrind": false,
  "iterations": 5,
  "variants": {
    "control": {
      "env": {
        "PROFILER": ""
      }
    },
    "with-cpu-profiler": {
      "env": {
        "PROFILER": "wall"
      }
    },
    "with-space-profiler": {
      "env": {
        "PROFILER": "space"
      }
    },
    "with-all-profilers": {
      "env": {
        "PROFILER": "all"
      }
    }
  }
}
