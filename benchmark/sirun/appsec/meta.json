{
  "name": "appsec",
  "cachegrind": false,
  "iterations": 24,
  "variants": {
    "control": {
      "setup": "bash -c \"nohup node client.js >/dev/null 2>&1 &\"",
      "run": "node server.js",
      "run_with_affinity": "bash -c \"taskset -c $CPU_AFFINITY node server.js\"",
      "env": {
        "DD_APPSEC_ENABLED": "0"
      }
    },
    "appsec-enabled": {
      "setup": "bash -c \"nohup node client.js >/dev/null 2>&1 &\"",
      "run": "node server.js",
      "run_with_affinity": "bash -c \"taskset -c $CPU_AFFINITY node server.js\"",
      "baseline": "control",
      "env": {
        "DD_APPSEC_ENABLED": "1"
      }
    },
    "control-with-attacks": {
      "setup": "bash -c \"nohup node client.js >/dev/null 2>&1 &\"",
      "run": "node server.js",
      "run_with_affinity": "bash -c \"taskset -c $CPU_AFFINITY node server.js\"",
      "env": {
        "DD_APPSEC_ENABLED": "0",
        "ATTACK_UA": "1",
        "ATTACK_404": "1",
        "ATTACK_QS": "1"
      }
    },
    "appsec-enabled-with-attacks": {
      "setup": "bash -c \"nohup node client.js >/dev/null 2>&1 &\"",
      "run": "node server.js",
      "run_with_affinity": "bash -c \"taskset -c $CPU_AFFINITY node server.js\"",
      "baseline": "control-with-attacks",
      "env": {
        "DD_APPSEC_ENABLED": "1",
        "ATTACK_UA": "1",
        "ATTACK_404": "1",
        "ATTACK_QS": "1"
      }
    }
  }
}
