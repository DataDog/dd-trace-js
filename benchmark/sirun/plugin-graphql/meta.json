{
  "name": "plugin-graphql",
  "run": "node index.js",
  "cachegrind": true,
  "iterations": 3,
  "variants": {
    "control": {},
    "with-async-hooks": {
      "baseline": "control",
      "env": { "WITH_ASYNC_HOOKS": "1" }
    },
    "with-depth-off": {
      "baseline": "control",
      "env": { "WITH_TRACER": "1", "WITH_DEPTH": "0"},
      "cachegrind": false
    },"with-depth-on-max": {
      "baseline": "control",
      "env": { "WITH_TRACER": "1", "WITH_DEPTH": "4"},
      "cachegrind": false
    },
    "with-depth-and-collapse-off": {
      "baseline": "control",
      "env": { "WITH_TRACER": "1", "WITH_DEPTH_AND_COLLAPSE": "4,0"},
      "cachegrind": false
    },
    "with-depth-and-collapse-on": {
      "baseline": "control",
      "env": { "WITH_TRACER": "1", "WITH_DEPTH_AND_COLLAPSE": "4,1"},
      "cachegrind": false
    }
  }
}
