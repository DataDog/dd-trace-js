{
  "name": "plugin-q",
  "run": "node index.js",
  "run_with_affinity": "bash -c \"taskset -c $CPU_AFFINITY node index.js\"",
  "cachegrind": false,
  "iterations": 80,
  "variants": {
    "control": {
      "env": {
        "USE_TRACER": "0"
      }
    },
    "with-tracer": {
      "baseline": "control",
      "env": {
        "USE_TRACER": "1"
      }
    }
  }
}
