{
  "extends": [
    "../.eslintrc.json"
  ],
  "env": {
    "mocha": true
  },
  "globals": {
    "expect": true,
    "sinon": true,
    "proxyquire": true,
    "withNamingSchema": true,
    "withVersions": true,
    "withExports": true,
    "withPeerService": true
  },
  "rules": {
    "no-unused-expressions": 0,
    "handle-callback-err": 0
  }
}
