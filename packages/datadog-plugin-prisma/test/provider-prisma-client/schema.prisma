generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
  url      = "postgres://postgres:postgres@localhost:5432/postgres"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
