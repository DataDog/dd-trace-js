generator client {
  provider = "prisma-client"
  output = env("PRISMA_CLIENT_OUTPUT")
  moduleFormat = "cjs"
  importFileExtension = "js"
}

datasource primary {
  provider = "postgresql"
  url      = env("PRISMA_TEST_DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
