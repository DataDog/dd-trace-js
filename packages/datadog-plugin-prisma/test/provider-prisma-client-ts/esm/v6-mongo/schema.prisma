generator client {
  provider = "prisma-client"
  output = env("PRISMA_CLIENT_OUTPUT")
  moduleFormat = "esm"
  importFileExtension = "js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  email String  @unique
  name  String?
}
