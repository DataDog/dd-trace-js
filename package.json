{
  "name": "dd-trace",
  "version": "0.5.5",
  "description": "Datadog APM tracing client for JavaScript",
  "main": "index.js",
  "scripts": {
    "bench": "node benchmark",
    "jsdoc": "gulp jsdoc",
    "jsdoc:watch": "gulp jsdoc:watch",
    "lint": "eslint . && node scripts/check_licenses.js",
    "tdd": "node ./scripts/install_plugin_modules && mocha --watch",
    "test": "node ./scripts/install_plugin_modules && nyc --reporter text --reporter lcov mocha 'test/**/*.spec.js'",
    "leak": "node --no-warnings ./node_modules/.bin/tape 'test/leak/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DataDog/dd-trace-js.git"
  },
  "keywords": [
    "datadog",
    "trace",
    "tracing",
    "opentracing",
    "apm"
  ],
  "author": "Datadog Inc. <info@datadoghq.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/DataDog/dd-trace-js/issues"
  },
  "homepage": "https://github.com/DataDog/dd-trace-js#readme",
  "engines": {
    "node": "^4.7 || ^6.9 || >=8"
  },
  "dependencies": {
    "async-hook-jl": "^1.7.6",
    "int64-buffer": "^0.1.9",
    "koalas": "^1.0.2",
    "lodash.kebabcase": "^4.1.1",
    "lodash.memoize": "^4.1.2",
    "lodash.uniq": "^4.5.0",
    "methods": "^1.1.2",
    "msgpack-lite": "^0.1.26",
