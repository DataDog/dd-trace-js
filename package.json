    "graphql": "^0.13.2",
    "gulp": "^3.9.1",
    "gulp-jsdoc3": "^2.0.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "mongodb-core": "^3.0.7",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "nock": "^9.6.1",
    "nyc": "^11.4.1",
    "pg": "^6.4.2",
    "proxyquire": "^1.8.0",
