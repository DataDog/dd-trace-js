    }
  },
  "devDependencies": {
    "@babel/helpers": "^7.27.6",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@msgpack/msgpack": "^3.1.2",
    "@openfeature/core": "^1.8.1",
    "@openfeature/server-sdk": "~1.18.0",
    "@stylistic/eslint-plugin": "^5.0.0",
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.106",
    "@types/sinon": "^17.0.4",
    "@types/tap": "^15.0.12",
    "axios": "^1.12.2",
    "benchmark": "^2.1.4",
    "body-parser": "^2.2.0",
    "chai": "^4.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-mocha": "^11.1.0",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "express": "^5.1.0",
    "glob": "^10.4.5",
    "globals": "^16.3.0",
    "graphql": "*",
    "jszip": "^3.10.1",
    "mocha": "^11.6.0",
    "multer": "^2.0.2",
    "nock": "^13.5.6",
    "nyc": "^15.1.0",
    "octokit": "^5.0.3",
    "proxyquire": "^2.1.3",
    "semver": "^7.7.2",
    "sinon": "^21.0.0",
    "sinon-chai": "^3.7.0",
    "tap": "^16.3.10",
