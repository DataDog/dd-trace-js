{
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": [
    "eslint:recommended",
    "standard"
  ],
  "env": {
    "node" : true,
    "browser": true
  },
  "rules": {
    "max-len": [2, 120, 2],
    "no-var": 2,
    "no-console": 2,
    "prefer-const": 2,
    "object-curly-spacing": [2, "always"]
  }
}
